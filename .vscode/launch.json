  "version": "0.2.0",
  "configurations":  [
    {
      "name": "Debug Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/venv/bin/python",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src/doc-gen"
      },
      "purpose": ["debug-test"],
      "presentation": {
        "hidden": false,
        "group": "RAQ.AI",
        "order": 1
      }
    }
  ]
}